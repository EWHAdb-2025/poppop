<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="400" prefWidth="600" spacing="10" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PopupSelectController">

    <children>
      <Label text="폐기물 처리 이력 입력">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label text="팝업스토어 선택">
         <font>
            <Font size="16.0" />
         </font>
      </Label>

        <TableView fx:id="popupTableView" prefHeight="300">
            <columns>
                <TableColumn fx:id="popupIdColumn" prefWidth="186.4000244140625" text="팝업명" />
                <TableColumn fx:id="popupNameColumn" prefWidth="198.39996337890625" text="팝업 종료 날짜" />
                <TableColumn fx:id="companyNameColumn" prefWidth="215.2000732421875" text="상호명" />
            </columns>
        </TableView>

        <HBox alignment="CENTER_RIGHT" spacing="10">
   <children>
               <Button fx:id="nextButton" alignment="TOP_LEFT" onAction="#handleNext" text="다음" />
   </children>
        </HBox>
    </children>
</VBox>
